						<string>// GET A REFERENCE TO THE KM ENGINE
var kme = Application("Keyboard Maestro Engine");

//--- GET A KM VARIABLE ---
var chordList = kme.getvariable('ChordList');

// Thank you hermo#4356 (CC Discord) for the help with this:
{
const dict = { 

// shift+1234567890
  '!': '1", "modifiers": "left_shift',
  '@': '2", "modifiers": "left_shift',
//  '#': '3", "modifiers": "left_shift', //needs to be saved for later
  '$': '4", "modifiers": "left_shift',
  '%': '5", "modifiers": "left_shift',
  '^': '6", "modifiers": "left_shift',
  '&amp;': '7", "modifiers": "left_shift',
  '*': '8", "modifiers": "left_shift',
  '(': '9", "modifiers": "left_shift',
  ')': '0", "modifiers": "left_shift',

// others
  '.': 'period',
  '&gt;': 'period", "modifiers": "left_shift',
  '/': 'slash',
  '?': 'slash", "modifiers": "left_shift',
  '&lt;': 'comma", "modifiers": "left_shift',
  '=': 'equal_sign',
  '+': 'equal_sign", "modifiers": "left_shift',
  '-': 'hyphen'

/* NOT WORKING
  '[': 'open_bracket',
  '{': 'open_bracket", "modifiers": "left_shift',
  ']': 'close_bracket',
  '}': 'close_bracket", "modifiers": "left_shift',
  '`': 'grave_accent_and_tilde'
*/

}
const re = new RegExp(`[${Object.keys(dict).join('')}]`,'g')

const replacedList = chordList.replace(re, char =&gt; dict[char])

kme.setvariable('chordList', { to: replacedList });

}

